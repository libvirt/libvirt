{
    "description": "SeaBIOS",
    "interface-types": [
        "bios"
    ],
    "mapping": {
        "device": "memory",
        "filename": "/usr/share/seabios/bios-256k.bin"
    },
    "targets": [
        {
            "architecture": "i386",
            "machines": [
                "pc-i440fx-*",
                "pc-q35-*"
            ]
        },
        {
            "architecture": "x86_64",
            "machines": [
                "pc-i440fx-*",
                "pc-q35-*"
            ]
        }
    ],
    "features": [
        "acpi-s3",
        "acpi-s4"
    ],
    "tags": [
        "CONFIG_BOOTSPLASH=n",
        "CONFIG_ROM_SIZE=256",
        "CONFIG_USE_SMM=n"
    ]
}
