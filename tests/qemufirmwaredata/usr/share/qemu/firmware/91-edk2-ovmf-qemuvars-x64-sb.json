{
    "description": "OVMF for qemu uefi-vars, SB disabled",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "memory",
        "filename": "/usr/share/edk2/ovmf/OVMF.qemuvars.fd",
        "uefi-vars": {
            "template": "/usr/share/edk2/ovmf/vars.blank.json"
        }
    },
    "targets": [
        {
            "architecture": "x86_64",
            "machines": [
                "pc-q35-*"
            ]
        }
    ],
    "features": [
        "acpi-s3",
        "secure-boot",
        "host-uefi-vars",
        "verbose-dynamic"
    ],
    "tags": [

    ]
}
