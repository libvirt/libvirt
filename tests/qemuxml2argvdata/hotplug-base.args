LC_ALL=C \
PATH=/bin \
HOME=/tmp/lib/domain--1-hotplug \
USER=test \
LOGNAME=test \
XDG_DATA_HOME=/tmp/lib/domain--1-hotplug/.local/share \
XDG_CACHE_HOME=/tmp/lib/domain--1-hotplug/.cache \
XDG_CONFIG_HOME=/tmp/lib/domain--1-hotplug/.config \
QEMU_AUDIO_DRV=none \
/usr/bin/qemu-system-x86_64 \
-name hotplug \
-S \
-machine pc,accel=kvm,usb=off,dump-guest-core=off \
-m 4096 \
-smp 4,sockets=4,cores=1,threads=1 \
-uuid d091ea82-29e6-2e34-3005-f02617b36e87 \
-display none \
-no-user-config \
-nodefaults \
-chardev socket,id=charmonitor,path=/tmp/lib/domain--1-hotplug/monitor.sock,\
server,nowait \
-mon chardev=charmonitor,id=monitor,mode=control \
-rtc base=utc \
-no-shutdown \
-device virtio-scsi-pci,id=scsi0,bus=pci.0,addr=0x3 \
-device virtio-serial-pci,id=virtio-serial0,bus=pci.0,addr=0x4 \
-usb
