LC_ALL=C \
PATH=/bin \
HOME=/home/test \
USER=test \
LOGNAME=test \
XAUTHORITY=/root/.Xauthority \
DISPLAY=:0.1 \
/usr/bin/qemu \
-name QEMUGuest1 \
-S \
-M pc \
-m 214 \
-smp 1 \
-uuid c7a5fdbd-edaf-9455-926a-d65c16db1809 \
-monitor unix:/tmp/test-monitor,server,nowait \
-no-acpi \
