msgid "  NAME\n"
msgstr "  പേര്\n"

#, c-format
msgid " %s (help keyword '%s')\n"
msgstr " %s (സഹായത്തിനുള്ള കീവേര്‍ഡ് '%s')\n"

#, c-format
msgid " %s (help keyword '%s'):\n"
msgstr " %s (സഹായത്തിനുള്ള കീബേര്‍ഡ് '%s'):\n"

